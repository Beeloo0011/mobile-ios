ci_hostname = ENV['CI_HOSTNAME']
raise "No project hostname" if ci_hostname.nil?
ci_token = ENV['CI_JOB_TOKEN']

if ci_token.nil?
    gem 'fastlane-plugin-pia_tools', git: "ssh://git@#{ci_hostname}/mobile/fastlane-plugin-pia_tools.git"
else
    gem 'fastlane-plugin-pia_tools', git: "https://gitlab-ci-token:#{ci_token}@#{ci_hostname}/mobile/fastlane-plugin-pia_tools.git"
end
